/* list.wxss */
.swim-wrap{
  width: 100%;
  height: 100%
}
.page-title{
  margin: 0 10px;
  height: 50px;
  line-height: 50px;
  font-size:17px;
  border-bottom: 1px #D3D3D3 solid;
}
.balance-item{
  float: left;
  font-size:14px;
}
.balance-item text {
  padding:0 5px;
  font-size:22px;
}
.balance-num{
  font-style: italic;
  color: #90EE90;
}

.page-title view image{
  width:30px;
  height:30px;
  float:right;
  margin-top:10px;
  margin-right:6px;
}
.swim-item{
  /*margin-top:15px;*/
  font-size:15px;
  padding:0 10px;
}
.checkbox{
  margin: 15px 0 0 10px;
  display: block;
}
.checkbox checkbox{
  margin-right: 5px; 
}
