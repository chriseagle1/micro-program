/* changebalance.wxss */
.wrap{
  height: 100px;
  padding: 0 10px;
}
.balance{
  height: 45px;
  line-height: 45px;
  border-bottom: 1px solid #D3D3D3;
}
.section {
  margin: 10px 0;
}
.recharge-title{
  font-size: 15px;
}
.recharge{
  margin:30px 0 20px 0;
  border-bottom:1px solid #D3D3D3;
  padding-bottom:30px;
}
/*.recharge image {
  width: 30px;
  height: 30px;
  float: left;
}*/

.recharge input{
  height:50px;
  margin-left:5px;
  line-height:50px;
  font-size:45px;
}

.confirm-charge button{
  background-color: #09BB07;
  color: #FFFFFF;
}




@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-demo22:before { content: "\e619"; }
.icon-chongdianjilu:before { content: "\e66a"; }

.money-icon {
  font-size:30px;
  color:#353535;
  float: left;
}
.history-charge {
  float:right;
  font-size:25px;
  color:#888888;
}