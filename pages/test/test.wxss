/* pages/test/test.wxss */
.program-button {
    background-color: #CDCDCD;
}

@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-demo22:before { content: "\e619"; }

.icon-mjiantou:before { content: "\e61e"; }

.icon-jiage-copy:before { content: "\e613"; }

.layer {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 20;
  background-color: #000;
  opacity: .3;
}
.date-select {
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  z-index: 40;
  background-color: #FFFFFF;
  /*padding-left: 20px;*/
}
.hide{
  display: none;
}
.show{
  display: block;
}

.date-select picker-view picker-view-column {
  text-align: center;
}